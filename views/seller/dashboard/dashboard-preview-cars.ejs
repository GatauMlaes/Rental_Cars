

<% if (data.cars.length !== 0) { %>
    <div class="content-wrapper">
        <div class="container-xxl flex-grow-1 container-p-y">
            <h6 class="pb-1 mb-4 text-muted"><%= title %></h6>
            <div class="row row-cols-1 row-cols-md-3 g-4 mb-5">

                <% for( let index = 0; index < data.cars.length; index++ ) { %>
                    <% const cars = data.cars[index] %>
                    <% const images = cars.images_cars %>
                    
                    <% if(images.length == 1) {%>
                        <div class="col">
                            <div class="card h-100">
                                <img class="card-img-top w-100" src="/Kelompok_3/<%= images.map((img) => img.path) %>" alt="Card image cap" />
                                <div class="card-body">
                                    <h5 class="card-title"><%= cars.merk %></h5>
                                    <p class="card-text">
                                        <%= cars.description %>
                                    </p>
                                </div>
                            </div>
                        </div>
                    <% } else { %>
                        <div class="col">
                            <div class="card">
                                <div id="carouselExampleIndicators2" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                        <div class="carousel-item active">
                                            <img class="w-100" src="/Kelompok_3/<%= images[0].path %>" alt="image">
                                        </div>
                                        <% for( let index = 0; index < images.length; index++ ) { %>
                                            <div class="carousel-item">
                                                <img class="w-100" src="/Kelompok_3/<%= images[index].path %>" alt="image">
                                            </div>
                                        <% } %>
                                    </div>
                                    <a class="carousel-control-prev" href="#carouselExampleIndicators2" role="button" data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carouselExampleIndicators2" role="button" data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                    <div class="card-body">
                                        <h5 class="card-title"><%= cars.merk %></h5>
                                        <p class="card-text">
                                            <%= cars.description %>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% } %>
                    <% } %>
            </div>
            <div class="row">
                <div class="col">
                    <div class="card">
                        <div class="col-lg-6 ">
                            <div class="demo-inline-spacing">
                              <!-- Basic Pagination -->
                              <nav aria-label="Page navigation">
                                <ul class="pagination pagination justify-content-end">
                                  <% const pages = data.page.totalPage %>
                                  <% for( let index = 0; index < pages; index++ ) { %>
                                    <li class="page-item">
                                      <a class="page-link" href="?p=<%= index + 1 %>"><i class="tf-icon"></i><%= index + 1 %></a>
                                    </li>
                                  <% } %>
                                </ul>
                    
                              </nav>
                              <!--/ Basic Pagination -->
                            </div>
                          </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

<% } else {%>

    <div class="container-xxl flex-grow-1 container-p-y">
        <div class="row">
            <div class="col">
                <div class="card">
                    <div class="d-flex align-items-end row">
                      <div class="col-sm-7">
                        <div class="card-body">
                          <h5 class="card-title text-primary">Your Data is Empty , <%= result.name %>! ðŸŽ‰</h5>
                          <p class="mb-4">
                            Start your business by managing your client data, and adding cars for rental
                          </p>
        
                          <a href="/Kelompok_3/seller/dashboard/add-cars/<%= result.id %>" class="btn btn-sm btn-outline-primary">Start</a>
                        </div>
                      </div>
                      <div class="col-sm-5 text-center text-sm-left">
                        <div class="card-body pb-0 px-0 px-md-4">
                          <img src="/Kelompok_3/img/illustrations/man.png" height="140" alt="View Badge User" data-app-dark-img="illustrations/man-with-laptop-dark.png" data-app-light-img="illustrations/man-with-laptop-light.png">
                        </div>
                      </div>
                    </div>
                  </div>
            </div>
        </div>
    </div>
<% } %>

<script>

</script>